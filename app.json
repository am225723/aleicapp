{
  "expo": {
    "name": "ALEIC",
    "slug": "aleic",
    "version": "1.0.0",
    "description": "Couples therapy app for relationship growth",
    "primaryColor": "#C9A962",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aleic",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A1C"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aleic.app",
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1A1A1C",
        "dark": {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#1A1A1C"
        },
        "tablet": {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#1A1A1C"
        }
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1A1A1C",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aleic.app",
      "versionCode": 1,
      "permissions": [],
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#1A1A1C"
      }
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-secure-store"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "b1d331a0-d4cf-4d7a-8749-ea73f1c18365"
      }
    }
  }
}
